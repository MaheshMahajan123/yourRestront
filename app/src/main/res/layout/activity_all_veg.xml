<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".Veg.All_veg">

    <androidx.cardview.widget.CardView
        android:layout_width="448dp"
        android:layout_height="732dp"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/Allveg"
            android:layout_width="399dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="70dp"
                android:layout_height="40dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/discription"
                app:layout_constraintVertical_bias="0.078"
                app:srcCompat="@mipmap/untitled" />

            <ImageView
                android:id="@+id/mImageView"
                android:layout_width="363dp"
                android:layout_height="242dp"
                android:layout_marginTop="100dp"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="402dp"
                android:layout_height="61dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/alex_brush"
                android:text="Veg- Item"
                android:textAlignment="center"
                android:textColor="@color/Brown"
                android:textSize="50dp"
                android:textStyle="bold|italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/price"
                android:layout_width="78dp"
                android:layout_height="76dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="112dp"
                android:background="@drawable/roundbutton"
                android:paddingLeft="20dp"
                android:paddingTop="30dp"
                android:text="222"
                android:textColor="@color/Black"
                android:textSize="23sp"
                android:textStyle="bold|italic"
                app:layout_constraintBottom_toTopOf="@+id/mEditTextFileName"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="24dp"
                android:layout_height="37dp"
                android:layout_marginTop="304dp"
                android:text="â‚¹"
                android:textColor="@color/Black"
                android:textSize="26sp"
                app:layout_constraintEnd_toEndOf="@+id/price"
                app:layout_constraintHorizontal_bias="0.444"
                app:layout_constraintStart_toStartOf="@+id/price"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/mEditTextFileName"
                android:layout_width="270dp"
                android:layout_height="37dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="32dp"
                android:fontFamily="sans-serif-smallcaps"
                android:text="Canada"
                android:textColor="#000"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.09"
                app:layout_constraintStart_toEndOf="@+id/price"
                app:layout_constraintTop_toBottomOf="@+id/mImageView" />

            <TextView
                android:id="@+id/discription"
                android:layout_width="364dp"
                android:layout_height="91dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="110dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="sans-serif-thin"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:textSize="22dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mEditTextFileName"
                app:layout_constraintVertical_bias="0.025" />

            <TextView
                android:id="@+id/eye"
                android:layout_width="200dp"
                android:layout_height="39dp"
                android:layout_marginStart="36dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="31dp"
                android:fontFamily="serif"
                android:text="View To Cart-->>"
                android:textColor="@color/Brown"
                android:textSize="22dp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toStartOf="@+id/imageView3"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintStart_toEndOf="@+id/addtocart"
                app:layout_constraintTop_toBottomOf="@+id/discription" />

            <TextView
                android:id="@+id/addtocart"
                android:layout_width="106dp"
                android:layout_height="45dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="288dp"
                android:background="@drawable/addtocart"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Add  +"
                android:textColor="@color/DarkGreen"
                android:textSize="25dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.15"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/discription"
                app:layout_constraintVertical_bias="0.011" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="190dp"
                android:layout_height="70dp"
                android:layout_marginStart="59dp"
                android:layout_marginTop="44dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:text="TextView"
                android:textSize="29dp"
                app:layout_constraintBottom_toBottomOf="@+id/discription"
                app:layout_constraintEnd_toEndOf="@+id/mEditTextFileName"
                app:layout_constraintHorizontal_bias="0.338"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mImageView"
                app:layout_constraintVertical_bias="1.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>